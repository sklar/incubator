{
    "name": "incubator",
    "version": "0.1.0",
    "description": "Simple scratchpad serving as a local Codepen alternative",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sklar/incubator.git"
    },
    "scripts": {
        "build": "webpack --env.production",
        "bump": "npm run bump:patch",
        "bump:major": "npm version major -m 'Bump v%s :raised_hands:'",
        "bump:minor": "npm version minor -m 'Bump v%s :point_up:''",
        "bump:patch": "npm version patch -m 'Bump v%s'",
        "clean": "rimraf public",
        "start": "webpack-dev-server --progress",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "autoprefixer": "^9.7.4",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "pug": "^2.0.4",
        "pug-loader": "^2.4.0",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "dependencies": {
        "normalize.css": "^8.0.1"
    },
    "browserslist": [
        "last 2 versions",
        "ie > 10"
    ],
    "engines": {
        "node": ">=10.0",
        "yarn": ">=1.15"
    },
    "private": true
}
